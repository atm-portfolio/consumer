{
  "name": "vue-portfolio-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:preview": "npm run build && vite preview",
    "build": "vue-tsc -b && vite build && cp _redirects dist/_redirects",
    "clear:npm": "rm -rf node_modules package-lock.json",
    "deploy": "tsc -b && vite build && sh post-build.sh",
    "lint": "eslint --fix",
    "start": "vite",
    "test": "playwright test"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "axios": "^1.9.0",
    "mitt": "^3.0.1",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.30",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.1.0",
    "prettier": "3.5.3",
    "sass": "^1.89.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  },
  "volta": {
    "node": "22.14.0"
  }
}
